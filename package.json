{
  "name": "redux-observable",
  "version": "0.1.0",
  "description": "Base boilerplate to create npm modules using typescript",
  "homepage": "https://github.com/beckend/typescript-npm-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beckend/typescript-npm-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/beckend/typescript-npm-boilerplate/issues"
  },
  "files": [
    "build-es5",
    "build-es2015",
    "build-es2017"
  ],
  "main": "./build-es5/index.js",
  "jsnext:main": "./build-es2015/index.js",
  "typings": "./build/index.d.ts",
  "scripts": {
    "prepare": "node_modules/.bin/gulp",
    "test": "node_modules/.bin/jest",
    "test:watch": "node_modules/.bin/jest --watchAll",
    "test:coverage": "node_modules/.bin/jest --coverage --no-cache",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(spec))\\.(ts|tsx)$",
    "collectCoverage": false,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "lcovonly",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "__TS_CONFIG__": "./src/tsconfig-test.json"
    }
  },
  "keywords": [
    "boilerplate",
    "typescript",
    "react",
    "npm"
  ],
  "author": "Linken Dinh",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.8",
    "@types/chai": "^4.0.1",
    "@types/debug": "0.0.29",
    "@types/fs-extra": "^3.0.3",
    "@types/gulp": "^4.0.4",
    "@types/gulp-debug": "^2.0.30",
    "@types/gulp-filter": "^3.0.30",
    "@types/gulp-typescript": "^2.13.0",
    "@types/jest": "^20.0.2",
    "@types/react": "^15.0.34",
    "@types/react-dom": "^15.5.1",
    "@types/redux": "^3.6.31",
    "@types/redux-actions": "^1.2.6",
    "@types/sinon": "^2.3.2",
    "bluebird": "^3.5.0",
    "chai": "^4.0.2",
    "cmd-spawn": "^1.4.0",
    "coveralls": "^2.13.1",
    "debug": "^2.6.8",
    "fs-extra": "^3.0.1",
    "globby": "^6.1.0",
    "google-closure-compiler-js": "^20170626.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-debug": "^3.1.0",
    "gulp-run": "^1.7.1",
    "gulp-sourcemap": "^1.0.1",
    "gulp-tslint": "^8.1.1",
    "gulp-typescript": "^3.1.7",
    "jest": "^20.0.4",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.1",
    "rxjs": "^5.4.2",
    "sinon": "^2.3.6",
    "ts-jest": "^20.0.6",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "tslint-react": "^3.0.0",
    "tslint-stylish": "^2.1.0",
    "typescript": "^2.4.1"
  },
  "peerDependencies": {
    "redux": "3.*",
    "rxjs": "^5.0.0-rc4"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
