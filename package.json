{
  "name": "redux-observable",
  "version": "0.1.0",
  "description": "Base boilerplate to create npm modules using typescript",
  "homepage": "https://github.com/beckend/typescript-npm-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beckend/typescript-npm-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/beckend/typescript-npm-boilerplate/issues"
  },
  "files": [
    "build-es5",
    "build-es2015",
    "build-es2017"
  ],
  "main": "./build-es5/index.js",
  "jsnext:main": "./build-es2015/index.js",
  "types": "./build-es2015/index.d.ts",
  "scripts": {
    "prepare-manual": "gulp",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --no-cache",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(spec))\\.(ts|tsx)$",
    "collectCoverage": false,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "lcovonly",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./src/tsconfig-test.json"
      }
    },
    "mapCoverage": true
  },
  "keywords": [
    "boilerplate",
    "typescript",
    "react",
    "npm"
  ],
  "author": "Linken Dinh",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.12",
    "@types/chai": "^4.0.4",
    "@types/debug": "0.0.30",
    "@types/fs-extra": "^4.0.2",
    "@types/gulp": "^4.0.4",
    "@types/gulp-debug": "^2.0.30",
    "@types/gulp-filter": "^3.0.30",
    "@types/gulp-typescript": "^2.13.0",
    "@types/jest": "^21.1.1",
    "@types/react": "^16.0.7",
    "@types/react-dom": "^15.5.5",
    "@types/redux": "^3.6.31",
    "@types/redux-actions": "^2.2.2",
    "@types/sinon": "^2.3.5",
    "bluebird": "^3.5.0",
    "chai": "^4.1.2",
    "cmd-spawn": "^1.4.0",
    "coveralls": "^2.13.1",
    "debug": "^3.1.0",
    "fs-extra": "^4.0.2",
    "globby": "^6.1.0",
    "google-closure-compiler-js": "^20170910.0.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-debug": "^3.1.0",
    "gulp-run": "^1.7.1",
    "gulp-sourcemap": "^1.0.1",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.2",
    "jest": "^21.2.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "redux": "^3.7.2",
    "rxjs": "^5.4.3",
    "sinon": "^4.0.0",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "tslint-stylish": "^2.1.0",
    "typescript": "^2.5.3"
  },
  "peerDependencies": {
    "redux": "3.*",
    "rxjs": "^5.*"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
